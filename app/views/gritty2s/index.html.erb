<h1>Listing gritty2s</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Time</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @gritty2s.each do |gritty2| %>
      <tr>
        <td><%= gritty2.title %></td>
        <td><%= gritty2.time %></td>
        <td><%= link_to 'Show', gritty2 %></td>
        <td><%= link_to 'Edit', edit_gritty2_path(gritty2) %></td>
        <td><%= link_to 'Destroy', gritty2, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Gritty2', new_gritty2_path %>
